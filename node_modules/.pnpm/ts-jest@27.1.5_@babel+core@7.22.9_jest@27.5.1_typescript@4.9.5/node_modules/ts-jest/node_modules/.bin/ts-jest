#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.22.9_jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.22.9_jest@27.5.1_typescript@4.9.5/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.22.9_jest@27.5.1_typescript@4.9.5/node_modules/ts-jest/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/ts-jest@27.1.5_@babel+core@7.22.9_jest@27.5.1_typescript@4.9.5/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli.js" "$@"
else
  exec node  "$basedir/../../cli.js" "$@"
fi
