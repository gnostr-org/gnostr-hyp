#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/hyperbeam@1.1.1/node_modules/hyperbeam/node_modules:/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/hyperbeam@1.1.1/node_modules:/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/hyperbeam@1.1.1/node_modules/hyperbeam/node_modules:/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/hyperbeam@1.1.1/node_modules:/Users/Shared/gnostr.org/hyper-cli/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin.js" "$@"
else
  exec node  "$basedir/../../bin.js" "$@"
fi
