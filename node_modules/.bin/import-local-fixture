#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/import-local/fixtures/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/import-local/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules:/Users/Shared/gnostr.org/gnostr/deps/node_modules:/Users/Shared/gnostr.org/gnostr/node_modules:/Users/Shared/gnostr.org/node_modules:/Users/Shared/node_modules:/Users/node_modules:/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/import-local/fixtures/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/import-local/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules:/Users/Shared/gnostr.org/gnostr/deps/node_modules:/Users/Shared/gnostr.org/gnostr/node_modules:/Users/Shared/gnostr.org/node_modules:/Users/Shared/node_modules:/Users/node_modules:/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
