#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules/standard/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules/standard/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules/standard/bin/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules/standard/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/standard@17.1.0/node_modules:/Users/Shared/gnostr.org/gnostr/deps/gnostr-hyp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../standard/bin/cmd.cjs" "$@"
else
  exec node  "$basedir/../standard/bin/cmd.cjs" "$@"
fi
