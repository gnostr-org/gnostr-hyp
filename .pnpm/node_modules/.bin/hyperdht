#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/Shared/gnostr.org/hyper-nostr/.pnpm/hyperdht@6.5.3/node_modules/hyperdht/node_modules:/Users/Shared/gnostr.org/hyper-nostr/.pnpm/hyperdht@6.5.3/node_modules:/Users/Shared/gnostr.org/hyper-nostr/.pnpm/node_modules"
else
  export NODE_PATH="/Users/Shared/gnostr.org/hyper-nostr/.pnpm/hyperdht@6.5.3/node_modules/hyperdht/node_modules:/Users/Shared/gnostr.org/hyper-nostr/.pnpm/hyperdht@6.5.3/node_modules:/Users/Shared/gnostr.org/hyper-nostr/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../hyperdht/bin.js" "$@"
else
  exec node  "$basedir/../hyperdht/bin.js" "$@"
fi
